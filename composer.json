{
  "name": "cakephpmitra/dbconfig",
  "description": "Store and manage application configuration in database for CakePHP 5.",
  "keywords": [
    "cakephp",
    "plugin",
    "config",
    "database",
    "settings"
  ],
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Atul Mahankal",
      "email": "atulmahankal@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "5.*"
  },
  "autoload": {
    "psr-4": {
      "DbConfig\\": "src/"
    }
  },
  "extra": {
      "cakephp-plugin": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
