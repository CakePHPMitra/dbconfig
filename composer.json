{
  "name": "cakephpmitra/dbconfig",
  "description": "Store and manage application configuration in database for CakePHP 5.",
  "keywords": [
    "cakephp",
    "plugin",
    "config",
    "database",
    "settings",
    "encryption"
  ],
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Atul Mahankal",
      "email": "atulmahankal@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "DbConfig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DbConfig\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs-check": "phpcs --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
    "cs-fix": "phpcbf --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/"
  },
  "extra": {
    "cakephp-plugin": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
